<BorderPane xmlns:fx="http://javafx.com/fxml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	
	<center>
		
		<GridPane fx:id="genMyModel">
			
			<children>
				
				<Label fx:id="density" GridPane.rowIndex="1" GridPane.columnIndex="1" />
				
				<Label fx:id="densityLabel" GridPane.rowIndex="1" GridPane.columnIndex="0" text="%Gaz.density" />
				
				<Label fx:id="name" GridPane.rowIndex="3" GridPane.columnIndex="1" />
				
				<Label fx:id="nameLabel" GridPane.rowIndex="3" GridPane.columnIndex="0" text="%Gaz.name" />
				
				<Label fx:id="type" GridPane.rowIndex="4" GridPane.columnIndex="1" />
				
				<Label fx:id="typeLabel" GridPane.rowIndex="4" GridPane.columnIndex="0" text="%Gaz.type" />
				
				<Label fx:id="weight" GridPane.rowIndex="5" GridPane.columnIndex="1" />
				
				<Label fx:id="weightLabel" GridPane.rowIndex="5" GridPane.columnIndex="0" text="%Gaz.weight" />
				
			</children>
		</GridPane>
	</center>
	<right>
		
		<Button text="Delete" onAction="#delete" />
		
	</right>
