<BorderPane xmlns:fx="http://javafx.com/fxml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	
	<center>
		
		<GridPane fx:id="genMyModel">
			
			<children>
				
				<TextField fx:id="name" GridPane.rowIndex="3" GridPane.columnIndex="1" editable="true" />
				
				<Label fx:id="nameLabel" GridPane.rowIndex="3" GridPane.columnIndex="0" text="%BottleType.name" />
				
				<TextField fx:id="volume" GridPane.rowIndex="4" GridPane.columnIndex="1" editable="true" />
				
				<Label fx:id="volumeLabel" GridPane.rowIndex="4" GridPane.columnIndex="0" text="%BottleType.volume" />
				
				<TextField fx:id="weight" GridPane.rowIndex="5" GridPane.columnIndex="1" editable="true" />
				
				<Label fx:id="weightLabel" GridPane.rowIndex="5" GridPane.columnIndex="0" text="%BottleType.weight" />
				
			</children>
		</GridPane>
	</center>
</BorderPane>
