<BorderPane xmlns:fx="http://javafx.com/fxml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	
	<center>
		
		<GridPane fx:id="genMyModel">
			
			<children>
				
				<TextField fx:id="load" GridPane.rowIndex="1" GridPane.columnIndex="1" editable="true" />
				
				<Label fx:id="loadLabel" GridPane.rowIndex="1" GridPane.columnIndex="0" text="%BottleConfiguration.load" />
				
				<TextField fx:id="quantity" GridPane.rowIndex="2" GridPane.columnIndex="1" editable="true" />
				
				<Label fx:id="quantityLabel" GridPane.rowIndex="2" GridPane.columnIndex="0" text="%BottleConfiguration.quantity" />
				
				<ChoiceBox fx:id="unit" GridPane.rowIndex="3" GridPane.columnIndex="1" />
				
				<Label fx:id="unitLabel" GridPane.rowIndex="3" GridPane.columnIndex="0" text="%BottleConfiguration.unit" />
				
			</children>
		</GridPane>
	</center>
</BorderPane>
