
# Entities and attributes
enum type --class ~.domain.ExerciseType
	enum constant --name CARDIO
	enum constant --name MUSCU
	

entity jpa --class ~.domain.MyUser  --sequenceName MYUSER_SEQ
	field string --fieldName username --notNull
	field string --fieldName email --notNull
	field string --fieldName Mypassword --notNull
	field number --fieldName Myid --type java.lang.Integer --primitive --notNull
	field date --fieldName signupDate --type java.util.Date --notNull
	
entity jpa --class ~.domain.SessionProgram  --sequenceName SESSIONPROGRAM_SEQ
	
entity jpa --class ~.domain.Exercise  --sequenceName EXERCISE_SEQ
	field enum --fieldName Mytype --type ~.domain.ExerciseType --notNull
	field number --fieldName Myid --type java.lang.Integer --primitive --notNull
	field string --fieldName Myname --notNull
	
entity jpa --class ~.domain.Sessions  --sequenceName SESSIONS_SEQ
	field date --fieldName Mydate --type java.util.Date --notNull
	field number --fieldName Myid --type java.lang.Integer --primitive --notNull
	


