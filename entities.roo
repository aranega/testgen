# References (relations between entities)
field set --fieldName sessionPrograms --type ~.domain.SessionProgram --class  ~.domain.MyUser --cardinality MANY_TO_MANY
field set --fieldName sessions --type ~.domain.Sessions --class  ~.domain.MyUser --cardinality MANY_TO_MANY
field set --fieldName exercises --type ~.domain.Exercise --class  ~.domain.SessionProgram --cardinality MANY_TO_MANY
field reference --fieldName sessionprogram --type ~.domain.SessionProgram --class  ~.domain.Sessions --cardinality MANY_TO_ONE


