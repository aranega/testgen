[comment encoding = UTF-8 /]
[module uml2test('http://www.eclipse.org/uml2/4.0.0/UML', 'http://www.eclipse.org/emf/2002/Ecore')/]


[template public generate(m : Model)]
[comment @main/]
[for (e : Element | m.ownedElement)]
[e.gen()/]
[/for]
[/template]


[template public gen(e : Element) /]
[template public gen(c : Class)]
[file (c.name.concat('.test'), false, 'UTF-8')]
  ## [c.name/] ##
  ###[c.name.replaceAll('.','#')/]###
[/file]
[/template] 

[template public myJavaService(aString : String)]
[invoke('java.lang.System', 'out.println()')/]
[/template]



 
